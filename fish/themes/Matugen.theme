
# Syntax highlighting colors
set -g fish_color_normal #0f1417
set -g fish_color_command #97dfff
set -g fish_color_quote #ffcc9a
set -g fish_color_redirection #234c5c
set -g fish_color_end #a4cce0
set -g fish_color_error #ffb4ab
set -g fish_color_param #bdc8cf
set -g fish_color_comment #879299
set -g fish_color_match #ffcc9a
set -g fish_color_selection--background=#234c5c
set -g fish_color_search_match --background=#234c5c
set -g fish_color_history_current --bold
set -g fish_color_operator #234c5c
set -g fish_color_escape #303638
set -g fish_color_cwd #97dfff
set -g fish_color_cwd_root #ffb4ab
set -g fish_color_valid_path --underline
set -g fish_color_autosuggestion #3d484e
set -g fish_color_user #ffcc9a
set -g fish_color_host #0f1417
