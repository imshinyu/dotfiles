set -g fish_pager_color_progress #303638 --background= #4cc7f3
set -g fish_pager_color_selected_background --background= #234c5c

set -g fish_color_option #bdc8cf
set -g fish_color_keyword #97dfff
set -g fish_color_host_remote #ffcc9a
set -g fish_color_status #ffb4ab
