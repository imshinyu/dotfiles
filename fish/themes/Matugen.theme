set -g fish_pager_color_progress #343438 --background= #818fbf
set -g fish_pager_color_selected_background --background= #44485b

set -g fish_color_option #c6c6d0
set -g fish_color_keyword #b7c5f7
set -g fish_color_host_remote #e4c27a
set -g fish_color_status #ffb4ab
