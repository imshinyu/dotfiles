
# Syntax highlighting colors
set -g fish_color_normal #131316
set -g fish_color_command #b7c5f7
set -g fish_color_quote #e4c27a
set -g fish_color_redirection #44485b
set -g fish_color_end #c1c5dc
set -g fish_color_error #ffb4ab
set -g fish_color_param #c6c6d0
set -g fish_color_comment #8f9099
set -g fish_color_match #e4c27a
set -g fish_color_selection--background=#44485b
set -g fish_color_search_match --background=#44485b
set -g fish_color_history_current --bold
set -g fish_color_operator #44485b
set -g fish_color_escape #343438
set -g fish_color_cwd #b7c5f7
set -g fish_color_cwd_root #ffb4ab
set -g fish_color_valid_path --underline
set -g fish_color_autosuggestion #45464e
set -g fish_color_user #e4c27a
set -g fish_color_host #131316
set -g fish_color_cancel -r

# Pager colors
set -g fish_pager_color_completion #131316
set -g fish_pager_color_description #c6c6d0 #e4c27a
